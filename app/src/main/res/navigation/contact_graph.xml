<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contact_graph"
    app:startDestination="@id/contact">

    <fragment
        android:id="@+id/contact"
        android:name="com.SE114PMCL.chatMessenger.Contact"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" >
        <action
            android:id="@+id/action_contact_to_friendList"
            app:destination="@id/friendList"
            app:enterAnim="@android:anim/slide_in_left"
            app:popEnterAnim="@android:anim/slide_in_left" />
        <action
            android:id="@+id/action_contact_to_groupList"
            app:destination="@id/groupList"
            app:enterAnim="@android:anim/slide_in_left"
            app:popEnterAnim="@android:anim/slide_in_left" />
    </fragment>
    <fragment
        android:id="@+id/friendList"
        android:name="com.SE114PMCL.chatMessenger.FriendList"
        android:label="fragment_friend_list"
        tools:layout="@layout/fragment_friend_list" >
        <action
            android:id="@+id/action_friendList_to_contact"
            app:destination="@id/contact" />
    </fragment>
    <fragment
        android:id="@+id/groupList"
        android:name="com.SE114PMCL.chatMessenger.GroupList"
        android:label="fragment_group_list"
        tools:layout="@layout/fragment_group_list" >
        <action
            android:id="@+id/action_groupList_to_contact"
            app:destination="@id/contact" />
    </fragment>
</navigation>